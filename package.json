    "express": "^4.17.1",
    "luxon": "^2.0.1",
    "nuxt": "2.15.7",
    "nuxt-property-decorator": "^2.9.1",
    "pageres": "^6.2.3",
    "screenshot-stream": "^4.2.0",
    "typed-vuex": "^0.2.0",
    "vue": "2.6.14",
    "vue-router": "3.5.2",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/eslint-parser": "7.14.7",
