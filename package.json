    "@nuxtjs/vuetify": "^1.12.1",
    "@types/luxon": "^1.27.1",
    "@types/node": "14.17.4",
    "eslint": "7.32.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-vue": "7.15.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-plugin-stylus": "0.11.0",
    "stylus": "0.54.8",
