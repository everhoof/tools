{
  "name": "@everhoof/tools",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12.14.1"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.js,.vue\" --ignore-path .gitignore .",
    "lint:styles": "npx stylelint \"**/*.{css,styl,scss,sass,vue}\" --custom-syntax stylelint-plugin-stylus/custom-syntax --allow-empty-input",
    "lint": "yarn lint:js && yarn lint:styles",
    "format": "yarn lint:js --fix && yarn lint:styles --fix",
    "mc": "node ./scripts/make-component.js"
  },
  "dependencies": {
